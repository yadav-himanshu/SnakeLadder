<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.snakeladder.GamePageController"
            prefHeight="400.0" prefWidth="600.0">
    <ImageView fitWidth="500" fitHeight="500" layoutX="300" layoutY="0">
        <image>
            <Image url="@Snake_Ladder_Board.jpg"/>
        </image>
    </ImageView>
    <ImageView fitWidth="25" fitHeight="50" layoutX="250" layoutY="450">
        <image>
            <Image url="@Token1.png"/>
        </image>
    </ImageView>
    <ImageView fitWidth="25" fitHeight="50" layoutX="275" layoutY="450">
        <image>
            <Image url="@token2.png"/>
        </image>
    </ImageView>
    <ImageView fitWidth="65" fitHeight="65" layoutX="130" layoutY="100" onMouseClicked="#rolldice">
        <image>
            <Image url="@dice.png"/>
        </image>
    </ImageView>
    <Text fx:id="number" layoutX="160" layoutY="185"></Text>

</AnchorPane>
